<?xml version="1.0" encoding="UTF-8"?>
<skin xmlns="http://schemas.getenvy.com/Skin.xsd" version="1.0">

<!-- Envy UK English Skin File -->

<!-- Translation based on default en.xml r26 -->
<!-- github.com/GetEnvy/Envy/blob/master/Languages/en.xml -->

<!-- This skin template represents current Envy defaults. -->
<!-- Use this file as a basis for creating new languages. -->
<!-- Only include changed sections, not unedited English. -->
<!-- Better concise than literal, please test visibility. -->

<!-- Note: Neighbour/Organise+ translated automatically in text. -->

<!-- Manifest -->
<manifest
	name="English"
	author="Envy Development Team"
	description="Envy Alt English Skin File"
	link="http://getenvy.com"
	email="Contact(at)GetEnvy.com"
	version="1.0"
	type="Language"
	language="en-uk"
	dir="ltr"
	priority="2"
	prompt="Click here to select English (UK) as your natural language."
/>

<!-- No Toolbar Definitions -->
	<!-- "Neighbour"+ translated automatically -->
<!-- Menu Definitions -->
	<!-- "Neighbour"+ translated automatically -->
<!-- Documents -->
<documents>
	<!-- Home View -->
	<document name="CHomeViewCtrl" leftMargin="20" topMargin="0">
		<styles>
			<style name="default">
				<font size="13"/>
			</style>
			<style name="heading">
				<font face="Segoe UI" size="18" weight="400"/>
			</style>
		</styles>
		<anchor id="Header" width="32" height="152"/>
		<newline gap="0"/>
		<group id="1">
			<icon res="136" width="32" height="32" target="command:ID_NETWORK_CONNECT"/><gap size="8"/>
			<heading align="middle">Get Connected</heading>
			<newline gap="8" indent="42"/>
			<text>Welcome to Envy.</text>
			<newline gap="8" indent="42"/>
			<text>You are not currently connected to any peer-to-peer networks.</text>
			<newline gap="2" indent="42"/>
			<link target="command:ID_NETWORK_CONNECT" format="bu">Click here to start</link>
			<newline gap="20" indent="0"/>
		</group>
		<group id="2">
			<icon res="130" width="32" height="32" target="command:ID_TAB_SEARCH"/><gap size="8"/>
			<heading align="middle">Search for Files</heading>
			<newline gap="4" indent="42"/>
			<anchor id="SearchBox" width="256" height="80"/>
			<newline gap="20" indent="0"/>
			<icon res="122" width="32" height="32" target="command:ID_TOOLS_DOWNLOAD"/><gap size="8"/>
			<heading align="middle">Start a Download</heading>
			<newline gap="8" indent="42"/>
			<link target="command:ID_TOOLS_DOWNLOAD" format="bu">Click here to start</link>
			<text align="middle">&#160;downloading a new file or torrent link.</text>
			<newline gap="4" indent="42"/>
			<text align="middle">Or open your completed </text>
			<link target="command:shell:downloads">downloads folder.</link>
			<newline gap="20" indent="0"/>
		</group>
		<group id="3">
			<icon res="120" width="32" height="32" target="command:upgrade"/><gap size="8"/>
			<heading align="middle">Upgrade Notice</heading>
			<newline gap="8" indent="42"/>
			<text id="Upgrade"/>
			<newline gap="6"/>
			<link target="command:upgrade">Click here to upgrade</link>
			<newline gap="18" indent="0"/>
		</group>
		<group id="5">
			<icon res="180" width="32" height="32" target="command:ID_TOOLS_WIZARD"/><gap size="8"/>
			<heading align="middle">Warning: You Are Firewalled</heading>
			<newline gap="8" indent="42"/>
			<text color="FF0000">Envy has detected that you are hidden behind a router or firewall,
			and are unable to receive active connections from the Internet. &#160;</text>
			<text color="FF0000" format="b">This may cause poor download and search performance.</text>
			<newline gap="6"/>
			<link target="command:ID_TOOLS_WIZARD">Enable Plug'n'Play port forwarding</link>
			<text>&#160;or </text>
			<link target="command:ID_HELP_ROUTER">learn how to configure your router.</link>
			<newline gap="20" indent="0"/>
		</group>
		<group id="6">
			<icon res="180" width="32" height="32"/><gap size="8"/>
			<heading align="middle">Warning: Your TCP Port Is Firewalled</heading>
			<newline gap="8" indent="42"/>
			<text color="FF0000">Envy has detected that you are behind a NAT router or firewall,
			and are unable to receive TCP connections from the Internet. &#160;</text>
			<text color="FF0000" format="b">This may cause poor download performance.</text>
			<newline gap="6"/>
			<link target="command:ID_TOOLS_WIZARD">Enable Plug'n'Play port forwarding</link>
			<text>&#160;or </text>
			<link target="command:ID_HELP_ROUTER">learn how to configure your router.</link>
			<newline gap="20" indent="0"/>
		</group>
		<group id="7">
			<icon res="180" width="32" height="32"/><gap size="8"/>
			<heading align="middle">Warning: Your UDP Port Is Firewalled</heading>
			<newline gap="8" indent="42"/>
			<text color="FF0000">Envy has detected that you are behind a NAT router or firewall,
			and are unable to receive UDP messages from the Internet. &#160;</text>
			<text color="FF0000" format="b">This may cause poor search performance.</text>
			<newline gap="6"/>
			<link target="command:ID_TOOLS_WIZARD">Enable Plug'n'Play port forwarding</link>
			<text>&#160;or </text>
			<link target="command:ID_HELP_ROUTER">learn how to configure your router.</link>
			<newline gap="20" indent="0"/>
		</group>
		<icon res="120" width="30" height="30" target="command:ID_TOOLS_SETTINGS"/><gap size="8"/>
		<heading align="middle">Customise Envy</heading>
		<newline gap="12" indent="42"/>
		<icon res="196" width="16" height="16" target="command:ID_TOOLS_SKIN"/><gap size="8"/>
		<link target="command:ID_TOOLS_SKIN" align="middle" format="bu">Skins</link><gap size="6"/>
		<text align="middle">- Mix&apos;n&apos;match changes to visual appearances</text>
		<newline gap="8"/>
		<icon res="139" width="16" height="16" target="command:ID_VIEW_SCHEDULER"/><gap size="8"/>
		<link target="command:ID_VIEW_SCHEDULER" align="middle">Scheduler</link><gap size="6"/>
		<text align="middle">- Set automated tasks to occur at specific times</text>
		<newline gap="8"/>
		<icon res="138" width="16" height="16" target="command:ID_VIEW_SECURITY"/><gap size="8"/>
		<link target="command:ID_VIEW_SECURITY" align="middle">Security Filters</link><gap size="6"/>
		<text align="middle">- Update blocklist to avoid bad search results</text>
		<newline gap="8"/>
		<icon res="197" width="16" height="16" target="command:ID_TOOLS_WIZARD"/><gap size="8"/>
		<link target="command:ID_TOOLS_WIZARD" align="middle">Settings Wizard</link><gap size="6"/>
		<text align="middle">- Quick-edit Envy's basic settings</text>
		<newline gap="8"/>
		<icon res="182" width="16" height="16" target="command:ID_HELP_FAQ"/><gap size="8"/>
		<link target="command:ID_HELP_FAQ" align="middle">Quick Help</link><gap size="6"/>
		<text align="middle">- Get online support using Envy, or try </text>
		<link target="command:ID_TAB_IRC" align="middle">live chat.</link>
		<newline gap="8"/>
		<icon res="201" width="16" height="16" target="command:ID_HELP_FORUMS_LOCAL"/><gap size="8"/>
		<link target="command:ID_HELP_FORUMS_LOCAL" align="middle">Local Forums</link><gap size="6"/>
		<text align="middle">- Go to the local English language forum</text>
		<newline gap="18" indent="0"/>
		<group id="4">
			<icon res="120" width="32" height="32" target="command:ID_TOOLS_SETTINGS"/><gap size="8"/>
			<heading align="middle">Remote Access Enabled</heading>
			<newline gap="8" indent="42"/>
			<text>Control Envy using your web browser at:</text>
			<newline gap="6"/>
			<link id="RemoteAccessURL"/>
			<text>&#160; or &#160;</text>
			<link id="RemoteAccessLocal"/>
			<newline gap="18" indent="0"/>
		</group>
		<newline gap="16" indent="0"/>
	</document>
	<!-- Settings Pages -->
	<document name="CGeneralSettingsGroup" title="General" leftMargin="8" topMargin="8">
		<text format="b">Welcome to Envy's Settings Manager</text>
		<newline gap="16"/>
		<text>These option pages allow advanced users to customise how Envy works.
			&#160;Settings in the </text>
		<text format="b">General </text>
		<text>section relate to the user experience.</text>
		<newline gap="16" indent="16"/>
		<link target="page:CGeneralSettingsPage" format="b">General:</link>
		<text>&#160;Common user interface settings like tooltips, minimise behavior, number formats, prompts, etc.</text>
		<newline gap="10"/>
		<link target="page:CLibrarySettingsPage" format="b">Library:</link>
		<text>&#160;Presentation, file access, and management of the file library.</text>
		<newline gap="10"/>
		<link target="page:CMediaSettingsPage" format="b">Media Player:</link>
		<text>&#160;Set when to use the internal Envy Media Center.</text>
		<newline gap="10"/>
		<link target="page:CIRCSettingsPage" format="b">IRC Chat:</link>
		<text>&#160;IRC chatroom connection settings and display options.</text>
		<newline gap="10"/>
		<link target="page:CCommunitySettingsPage" format="b">Community:</link>
		<text>&#160;Your profile and direct chat settings.</text>
		<newline gap="10"/>
		<link target="page:CRemoteSettingsPage" format="b">Remote:</link>
		<text>&#160;Setup Envy to be controlled from a web browser.</text>
		<newline gap="10"/>
		<link target="page:CWebSettingsPage" format="b">Web Integration:</link>
		<text>&#160;Enable your web browser to use Envy for peer-to-peer links and as general download manager.</text>
	</document>
	<document name="CInternetSettingsGroup" title="Internet" leftMargin="8" topMargin="8">
		<text format="b">Welcome to Envy's Settings Manager</text>
		<newline gap="16"/>
		<text>These option pages allow advanced users to customise how Envy works.
			&#160;Settings in the </text>
		<text format="b">Internet </text>
		<text>section control how Envy interacts with other nodes in distributed peer-to-peer networks.</text>
		<newline gap="16" indent="16"/>
		<link target="page:CConnectionSettingsPage" format="b">Connection:</link>
		<text>&#160;Indicate speed and capabilities of your Internet access.</text>
		<newline gap="10"/>
		<link target="page:CDownloadsSettingsPage" format="b">Downloads:</link>
		<text>&#160;Manage bandwidth-in, limits, and folder locations.</text>
		<newline gap="10"/>
		<link target="page:CUploadsSettingsPage" format="b">Uploads:</link>
		<text>&#160;Manage bandwidth-out, limits, and queues.</text>
		<newline gap="10"/>
		<link target="page:CSchedulerSettingsPage" format="b">Scheduler:</link>
		<text>&#160;Limit download connections based on a periodic schedule.</text>
	</document>
</documents>
<!-- Localised Command Tip Text."Message" displayed in status bar, "Tip" shown in tooltip -->
<commandtips>
	<!-- "Neighbour" translated automatically -->
	<tip id="ID_CHAT_COLOR" text="Select a text colour (Ctrl+K)"/>
	<tip id="ID_VIEW_NEIGHBOURS" text="Show or hide the NEIGHBOURS window."/>
</commandtips>
<!-- Localised Strings -->
<strings>
	<!-- "Neighbour" translated automatically -->
</strings>
<!-- Localised Dialog Text, "cookie" verifies that the skin matches current dialog -->
<dialogs>
	<!-- "Neighbour"+ translated automatically -->
	<dialog name="CFilesProfilePage" cookie="BuStSy" caption="Favourites">
		<control text="_Favourite Files:"/>
		<control text="Favourite files are recommended to users who browse your profile. You can create favourite files by adding them to the &quot;Favourites&quot; library folder."/>
		<control/>
	</dialog>
	<dialog name="CMediaVisDlg" cookie="BuSyCoBuBuBu" caption="Select Visualisation">
		<control text="Select an audio visualisation from the list:"/>
		<control/>
		<control text="Footer Display|Full Size Display|Large Display|Medium Display|Small Display"/>
		<control text="_Setup..."/>
		<control text="OK"/>
		<control text="Cancel"/>
	</dialog>
	<dialog name="CDecodeMetadataDlg" cookie="StCoBuBuBuStStBuBu" caption="Metadata Localisation">
		<control text="Please select the method of re-encoding:"/>
		<control text="Arabic|Baltic|Central European|Chinese Simplified|Chinese Traditional|Cyrillic|Greek|Hebrew|Japanese|Korean|Thai|Turkish|Vietnamese"/>
		<control text="Preview:"/>
		<control/>
		<control/>
		<control/>
		<control/>
		<control text="OK"/>
		<control text="Cancel"/>
	</dialog>
</dialogs>
<!-- No Column Definitions -->
</skin>
