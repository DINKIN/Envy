<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="http://schemas.getenvy.com/ImageFolder.xsd" xmlns="http://www.w3.org/2001/XMLSchema">

	<element name="imageFolders">
		<complexType>
			<element name="imageFolder" type="foldertype" minOccurs="1" maxOccurs="unbounded"/>
		</complexType>
	</element>
	<complexType name="foldertype">
		<all>
			<attribute name="title" type="string"/>
		</all>
	</complexType>

</schema>